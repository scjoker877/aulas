void setup()
{
pinMode(4, OUTPUT);
pinMode(2, OUTPUT);
pinMode(3, OUTPUT);
pinMode(5, OUTPUT);
pinMode(1, OUTPUT);
}

void loop()
{
  digitalWrite(4, HIGH);
  delay(2000); 
  digitalWrite(2, HIGH);
  delay(2000);
  digitalWrite(3, HIGH);
  delay(2000); 
  digitalWrite(5, HIGH);
  delay(2000);
  digitalWrite(1, HIGH);
  delay(2000);
  digitalWrite(4, LOW);digitalWrite(2, LOW);digitalWrite(3, LOW);digitalWrite(5, LOW);digitalWrite(1, LOW);
   delay(2000);
}
